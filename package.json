{
  "name": "webpack-starter",
  "version": "1.0.0",
  "description": "simple boilerplate to use webpack for building a client",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "webpack",
    "build:watch": "webpack  --watch",
    "build:docker": "npm run build && docker build -t iv247-webpack .",
    "start:docker": "docker run --name iv247-nginx -d -p 8080:80 iv247-webpack",
    "start:dev": "webpack-dev-server --progress --colors --inline --hot --port 3001",
    "open:dev": "node node_modules/opn-cli/cli http://localhost:3001/ || true",
    "dev": "concurrently \"npm run start:dev\" \"npm run open:dev\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/iv247/webpack.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/iv247/webpack/issues"
  },
  "homepage": "https://github.com/iv247/webpack#readme",
  "devDependencies": {
    "autoprefixer": "7.1.6",
    "babel-cli": "6.26.0",
    "babel-core": "6.26.0",
    "babel-eslint": "8.0.2",
    "babel-loader": "7.1.2",
    "babel-preset-env": "1.6.1",
    "babel-preset-es2015": "6.24.1",
    "babel-preset-stage-2": "6.24.1",
    "concurrently": "3.5.0",
    "copy-webpack-plugin": "4.2.1",
    "eslint": "4.11.0",
    "eslint-loader": "^1.9.0",
    "extract-text-webpack-plugin": "3.0.2",
    "file-loader": "1.1.5",
    "html-webpack-plugin": "2.30.1",
    "less": "2.7.3",
    "less-loader": "4.0.5",
    "mocha": "^5.0.0",
    "node-sass": "4.7.1",
    "nodemon": "1.12.1",
    "opn-cli": "3.1.0",
    "postcss-loader": "2.0.8",
    "sass-loader": "6.0.6",
    "style-loader": "0.19.0",
    "url-loader": "0.6.2",
    "webpack": "3.8.1",
    "webpack-dev-server": "2.9.4",
    "webpack-merge": "4.1.1"
  },
  "dependencies": {
    "gocanvas": "git://github.com/alanf718/GoCanvas.git"
  }
}
